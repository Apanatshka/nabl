module nabl2/shared/common/Identifiers

imports

  nabl2/shared/common/Native

lexical syntax

  INT            = "-"? [0-9]+

  TVarId         = [a-zA-Z] [a-zA-Z0-9]*
  TVarId         = Keyword {reject}

  TModId         = [a-z] [a-zA-Z0-9]*
  TModId         = Keyword {reject}

  VarId          = [a-z] [a-zA-Z0-9\_]* [\']*
  VarId          = [a-zA-Z0-9\_]* "_" {reject}
  VarId          = Keyword [\']* {reject}

  OpId           = [A-Z] [a-zA-Z0-9\_\']*
  OpId           = [A-Z0-9\_\']* "_" {reject}
  OpId           = Keyword {reject}
  OpId           = Constructor [\']* {reject}
  
  NamespaceId    = [A-Z] [a-zA-Z0-9]*
  NamespaceId    = Keyword {reject}

  KeyId          = [a-z] [a-zA-Z0-9]*
  Keyid          = Keyword {reject}

  LabelId        = [A-Z]+
  LabelId        = "D" | "I" | "P" {reject}

  Keyword        = ABSTRACT
  Constructor    = ABSTRACT
 
lexical restrictions

  Int          -/- [0-9]

  VarId        -/- [a-zA-Z0-9\_\']
  OpId         -/- [a-zA-Z0-9\_\']
  NamespaceId  -/- [a-zA-Z0-9]

